<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>conceptnamefinder</id>
	<name>Concept Name Finder Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.conceptnamefinder</package>
	<author>Okuboyejo Damilola</author>
	<description>
		Helps toConcept
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/conceptnamefinder/update.rdf</updateURL>
	<require_version>1.9.0</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.conceptnamefinder.ConceptNameFinderActivator</activator>
	
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.conceptnamefinder.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
		ConceptNameFinder.hbm.xml
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with conceptnamefinder. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

